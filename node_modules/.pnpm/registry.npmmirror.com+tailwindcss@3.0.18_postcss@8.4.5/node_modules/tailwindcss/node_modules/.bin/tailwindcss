#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/registry.npmmirror.com+tailwindcss@3.0.18_postcss@8.4.5/node_modules/tailwindcss/lib/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/registry.npmmirror.com+tailwindcss@3.0.18_postcss@8.4.5/node_modules/tailwindcss/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/registry.npmmirror.com+tailwindcss@3.0.18_postcss@8.4.5/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules:/Users/bt/Documents/code/node_modules:/Users/bt/Documents/node_modules:/Users/bt/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/registry.npmmirror.com+tailwindcss@3.0.18_postcss@8.4.5/node_modules/tailwindcss/lib/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/registry.npmmirror.com+tailwindcss@3.0.18_postcss@8.4.5/node_modules/tailwindcss/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/registry.npmmirror.com+tailwindcss@3.0.18_postcss@8.4.5/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules/.pnpm/node_modules:/Users/bt/Documents/code/tailwind-static-website/node_modules:/Users/bt/Documents/code/node_modules:/Users/bt/Documents/node_modules:/Users/bt/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
